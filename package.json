{
  "name": "artode-icons",
  "version": "0.1.0",
  "description": "A premium collection of canvas-based, animated icons designed with a Swiss International Style aesthetic.",
  "license": "MIT",
  "author": "adhd.dev <https://x.com/adhd_paws>",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "next dev",
    "prebuild": "node scripts/download-fonts.js",
    "build": "next build",
    "build:lib": "tsup",
    "start": "next start",
    "lint": "bun run check-style:fix && npx ultracite fix",
    "lint:check": "bun run check-style && npx ultracite check",
    "check-duplicates": "tsx scripts/check-duplicates.ts",
    "check-imports": "tsx scripts/check-imports.ts",
    "check-style": "tsx scripts/check-icon-style.ts",
    "check-style:fix": "tsx scripts/check-icon-style.ts --fix",
    "sync-registry": "tsx scripts/sync-registry.ts",
    "build-registry": "tsx scripts/registry-build.ts",
    "gen-cli": "bun run check-style:fix && bun run sync-registry && bun run build-registry && bun lint"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@openpanel/nextjs": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@toolwind/corner-shape": "0.0.8-3",
    "babel-plugin-react-compiler": "1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fuse.js": "^7.1.0",
    "geist": "^1.5.1",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "next": "^16.1.1",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.5",
    "react-hotkeys-hook": "^5.2.1",
    "simple-icons": "^16.4.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "postcss": "^8.5.6",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "ultracite": "7.0.5"
  }
}
